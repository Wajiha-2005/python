a=input("enter name1:")
b=input("enter name2:")
a1=list(a)
b1=list(b)
for i in range(len(a1)):
    for j in range(len(b1)):
        if a1[i]==b1[j]:
            a1[i]='2'
            b1[j]='2'
            print(a1)
            print(b1)
            count=0
for i in a1:
    if(i!='2'):
        count=count+1
for i in b1:
    if(i!='2'):
        count=count+1
print(count)
f=0
res=list("FLAMES")

while(len(res)!=1):
    f=(f+(count-1))%len(res)
    res.pop(f)
print(res[0])
